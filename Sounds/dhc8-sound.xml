<?xml version="1.0"?>
 
<PropertyList>

    <fx>
<!--Battery Switch, Cabincall, no smoking sign, seatbelt sign; code from 777, audio produced with audactiy-->
	<chime>
		<name>battery</name>
		<mode>once</mode>
		<type>avionics</type>
		<path>cabincall.wav</path>
		<condition>
		            <property>/controls/electric/battery-switch</property>
		</condition>
            <position>
                <x>-9</x>
                <y>0</y>
                <z>-0.5</z>
            </position>
	</chime>
	<chime>
		<name>nosmoking</name>
		<mode>once</mode>
		<type>avionics</type>
		<path>cabincall.wav</path>
		<condition>
			<and>
		            <property>/controls/electric/battery-switch</property>
				<or>
					<equals>
    	    	        <property>controls/cabin/nosmoking</property>
						<value>1</value>
					</equals>
					<equals>
    	    	        <property>controls/cabin/nosmoking</property>
						<value>0</value>
					</equals>
				</or>
			</and>
		</condition>
	</chime>	   
   
	<chime>
		<name>seatbelt</name>
		<mode>once</mode>
		<type>avionics</type>
		<path>cabincall.wav</path>
		<condition>
			<and>
		            <property>/controls/electric/battery-switch</property>
				<or>
					<equals>
    	    	        <property>controls/cabin/seatbelts</property>
						<value>1</value>
					</equals>
					<equals>
    	    	        <property>controls/cabin/seatbelts</property>
						<value>0</value>
					</equals>
				</or>
			</and>
		</condition>
	</chime>   
   
	<chime>
		<name>cabincall</name>
		<mode>once</mode>
		<type>avionics</type>
		<path>cabincall.wav</path>
		<condition>
			<and>
		            <property>/controls/electric/battery-switch</property>
					<equals>
    	    	        <property>controls/cabin/cabincall</property>
						<value>1</value>
					</equals>
			</and>
		</condition>
	</chime>
<!-- APU, same for inside and outside; APU sound and config from CRJ700-family licensed under GPL -->
		<apu>
			<name>APU starting</name>
			<mode>once</mode>
			<path>apu_start.wav</path>
			<condition>
					<property>controls/APU/start</property>
			</condition>
			<reference-dist>10</reference-dist>
			<max-dist>50</max-dist>
			<position>
				<x>13.30</x>
				<y>0.01</y>
				<z>-0.70</z>
			</position>
		</apu>
		<apu>
			<name>APU running</name>
			<mode>looped</mode>
			<path>/apu_run.wav</path>
			<condition>
					<property>controls/APU/running</property>
			</condition>
			<reference-dist>10</reference-dist>
			<max-dist>50</max-dist>
			<position>
				<x>13.30</x>
				<y>0.01</y>
				<z>-0.70</z>
			</position>
		</apu>
		<apu>
			<name>APU shutdown</name>
			<mode>once</mode>
			<path>apu_shutdown.wav</path>
			<condition>
				<and>
					<not>
						<property>controls/APU/master</property>
					</not>
				</and>
			</condition>
			<reference-dist>10</reference-dist>
			<max-dist>50</max-dist>
			<position>
				<x>13.30</x>
				<y>0.01</y>
				<z>-0.70</z>
			</position>
		</apu>
