<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

<path>pfd3d.ac</path>

    <animation>
        <name>ias ladder</name>
        <type>texmultiple</type>
        <object-name>iasladder</object-name>
        <transform>
            <subtype>textranslate</subtype>
            <property>instrumentation/airspeed-indicator/indicated-speed-kt</property>
            <interpolation>
                <entry><ind>0.0</ind><dep>0.0</dep></entry>
                <entry><ind>90.000</ind><dep>0.4536</dep></entry>
                <entry><ind>90.001</ind><dep>0.0</dep></entry>
                <entry><ind>180.000</ind><dep>0.4536</dep></entry>
                <entry><ind>180.001</ind><dep>0.0</dep></entry>
                <entry><ind>270.000</ind><dep>0.4536</dep></entry>
                <entry><ind>270.001</ind><dep>0.0</dep></entry>
                <entry><ind>360.000</ind><dep>0.4536</dep></entry>
                <entry><ind>360.001</ind><dep>0.0</dep></entry>
                <entry><ind>450.000</ind><dep>0.4536</dep></entry>
                <entry><ind>450.001</ind><dep>0.0</dep></entry>
                <entry><ind>540.000</ind><dep>0.4536</dep></entry>
            </interpolation>
            <axis>
                <x>0</x>
                <y>1</y>
            </axis>
        </transform>
        <transform>
            <subtype>textranslate</subtype>
            <property>instrumentation/airspeed-indicator/indicated-speed-kt</property>
            <interpolation>
                <entry><ind>0.0</ind><dep>0.0</dep></entry>
                <entry><ind>90.000</ind><dep>0.0</dep></entry>
                <entry><ind>90.001</ind><dep>0.11</dep></entry>
                <entry><ind>180.000</ind><dep>0.11</dep></entry>
                <entry><ind>180.001</ind><dep>0.22</dep></entry>
                <entry><ind>270.000</ind><dep>0.22</dep></entry>
                <entry><ind>270.001</ind><dep>0.33</dep></entry>
                <entry><ind>360.000</ind><dep>0.33</dep></entry>
                <entry><ind>360.001</ind><dep>0.44</dep></entry>
                <entry><ind>450.000</ind><dep>0.44</dep></entry>
                <entry><ind>450.001</ind><dep>0.55</dep></entry>
                <entry><ind>540.000</ind><dep>0.55</dep></entry>
            </interpolation>
            <axis>
                <x>1</x>
                <y>0</y>
                <z>0</z>
            </axis>
        </transform>
    </animation>
	<animation>
		<type>select</type>
		<object-name>display.off</object-name>
		<condition>
			<less-than>
				<property>/systems/electrical/batt-volts</property>
				<value>10</value>
			</less-than>
		</condition>
	</animation>

    <animation>
        <type>textranslate</type>
        <object-name>ias.d1</object-name>
        <property>instrumentation/airspeed-indicator/round-speed-kt</property>
        <factor>0.001</factor>
        <step>100</step>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>ias.d2</object-name>
        <property>instrumentation/airspeed-indicator/round-speed-kt</property>
