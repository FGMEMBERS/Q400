    </animation>

    <animation>
        <name>Attitude indicator</name>
        <type>texmultiple</type>
        <object-name>att</object-name>
        <transform>
            <subtype>textranslate</subtype>
            <property>orientation/pitch-deg</property>
            <interpolation>
              <entry><ind>-90.0</ind><dep>-0.50</dep></entry> <!--orig. -0.41-->
              <entry><ind>0.0</ind><dep>0.0</dep></entry>	<!-- orig. ind=0 -->
              <entry><ind>90.0</ind><dep>0.50</dep></entry> <!-- orig. 0.41 -->
            </interpolation>
            <axis>
                <x>0</x>
                <y>1</y>
            </axis>
        </transform>
        <transform>
            <subtype>texrotate</subtype>
            <property>orientation/roll-deg</property>
            <axis>
                <x>0</x>
                <y>0</y>
                <z>-1</z>
            </axis>
            <center>
                <x>0.25</x>
                <y>0.5</y>
            </center>
        </transform>
    </animation>

<!-- altitude cursor -->

    <animation>
      <object-name>AltitudeCursor</object-name>
			  <property>instrumentation/pfd/target-altitude-diff</property>
       <type>translate</type>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>1</z>
        </axis>
 		<!-- <factor>0.000068</factor> -->
				<interpolation>
					<entry>
						<ind>-615.0</ind>
						<dep>-0.04182</dep>
					</entry>
					<entry>
						<ind>0.0</ind>
						<dep>0.0</dep>
					</entry>
					<entry>
						<ind>615.0</ind>
						<dep>0.04182</dep>
					</entry>
				</interpolation>
    </animation>

<!-- Altitude Trend -->

    <animation>
        <object-name>alttrend.up</object-name>
        <property>instrumentation/pfd/alt-trend-up</property>
        <type>textranslate</type>
        <axis>
            <x>0</x>
            <y>1</y>
        </axis>
        <factor>1.2</factor> 			
    </animation>

    <animation>
        <object-name>alttrend.dwn</object-name>
        <property>instrumentation/pfd/alt-trend-down</property>
        <type>textranslate</type>
        <axis>
            <x>0</x>
            <y>1</y>
        </axis>
        <factor>1.2</factor>
    </animation>

    <animation>
        <type>select</type>
        <object-name>alttrend.up</object-name>
        <condition>
            <less-than>
                <property>instrumentation/pfd/alt-trend-up</property>
                <value>0.239</value>
            </less-than>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>alttrend.dwn</object-name>
        <condition>
            <greater-than>
                <property>instrumentation/pfd/alt-trend-down</property>
                <value>-0.239</value>
            </greater-than>
        </condition>
    </animation>

<!-- minimums -->

    <animation>
        <type>select</type>
        <object-name>min.head</object-name>
        <condition>
          <less-than-equals>
              <property>instrumentation/pfd/minimum-diff</property>
              <value>0</value>
          </less-than-equals>
					<greater-than-equals>
              <property>instrumentation/pfd/minimum-diff</property>
              <value>-600</value>
					</greater-than-equals>			
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>min.tail</object-name>
        <condition>
					<equals>
            <property>autopilot/settings/minimums-mode</property>
						<value>BA</value>
					</equals>
          <less-than-equals>
            <property>instrumentation/pfd/minimum-diff</property>
            <value>0</value>
          </less-than-equals>
					<greater-than-equals>
            <property>instrumentation/pfd/minimum-diff</property>
            <value>-600</value>
					</greater-than-equals>			
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>min.head1</object-name>
        <condition>
          <greater-than-equals>
              <property>instrumentation/pfd/minimum-diff</property>
              <value>0</value>
          </greater-than-equals>
          <less-than-equals>
            <property>instrumentation/pfd/minimum-diff</property>
            <value>600</value>
          </less-than-equals>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>min.tail1</object-name>
        <condition>
					<equals>
            <property>autopilot/settings/minimums-mode</property>
						<value>BA</value>
					</equals>
          <greater-than-equals>
              <property>instrumentation/pfd/minimum-diff</property>
              <value>0</value>
          </greater-than-equals>
          <less-than-equals>
            <property>instrumentation/pfd/minimum-diff</property>
            <value>600</value>
          </less-than-equals>
        </condition>
    </animation>

    <animation>
        <type>translate</type>
        <object-name>min.head</object-name>
        <object-name>min.tail</object-name>
        <object-name>min.head1</object-name>
        <object-name>min.tail1</object-name>
        <property>instrumentation/pfd/minimum-diff</property>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>1</z>
        </axis>
        <factor>0.000068</factor>
    </animation>

<!-- speed cursor -->

    <animation>
        <type>select</type>
        <object-name>SpeedCursor</object-name>
           <condition>
             <or>
              <equals>
                  <property>/autopilot/locks/altitude</property>
                  <value>FLC</value>
              </equals>
              <equals>
                  <property>/autopilot/locks/altitude</property>
                  <value>VFLC</value>
              </equals>
             </or>
           </condition>
    </animation>
		
    <animation>
      <object-name>SpeedCursor</object-name>
			  <property>instrumentation/pfd/target-speed-diff</property>
       <type>translate</type>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>1</z>
        </axis>
				<interpolation>
					<entry>
						<ind>-40</ind>
						<dep>-0.041</dep>
					</entry>
					<entry>
						<ind>0</ind>
						<dep>0</dep>
					</entry>
					<entry>
						<ind>40.0</ind>
						<dep>0.041</dep>
					</entry>
				</interpolation>

    </animation>

<!-- speed indication -->

    <animation>
        <type>select</type>
        <object-name>Vspeed</object-name>
        <object-name>IE-text</object-name>
        <object-name>IR-text</object-name>
        <object-name>IR-value</object-name>
        <object-name>I2-text</object-name>
        <object-name>I2-value</object-name>
        <object-name>I1-text</object-name>
        <object-name>I1-value</object-name>
        <condition>
					<and>
	          <property>gear/gear[1]/wow</property>
		        <less-than>
		            <property>velocities/airspeed-kt</property>
		            <value>30</value>
		        </less-than>
					</and>
        </condition>
    </animation>


<!-- Speed Trend -->

    <animation>
        <object-name>speedtrend.up</object-name>
        <property>instrumentation/pfd/speed-trend-up</property>
        <type>textranslate</type>
        <axis>
            <x>0</x>
            <y>1</y>
        </axis>
        <factor>1</factor> 
