        <factor>0.00092</factor>
    </animation>

<!-- Vf15 animation -->

    <animation>
        <type>select</type>
        <object-name>vf15-text</object-name>
        <condition>
					<not>
            <property>gear/gear[1]/wow</property>
					</not>
					<equals>
            <property>controls/flight/flaps</property>						
						<value>0.428</value>
					</equals>
          <less-than>
              <property>instrumentation/pfd/vf15-diff</property>
              <value>45</value>
          </less-than>
          <greater-than>
              <property>instrumentation/pfd/vf15-diff</property>
              <value>-45</value>
          </greater-than>
        </condition>
    </animation>

    <animation>
        <type>translate</type>
        <object-name>vf15-text</object-name>
        <property>instrumentation/pfd/vf15-diff</property>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>1</z>
        </axis>
        <factor>0.00092</factor>
    </animation>

<!-- Vf35 animation -->

    <animation>
        <type>select</type>
        <object-name>vf35-text</object-name>
        <condition>
					<not>
            <property>gear/gear[1]/wow</property>
					</not>
					<equals>
            <property>controls/flight/flaps</property>						
						<value>1</value>
					</equals>
          <less-than>
              <property>instrumentation/pfd/vf35-diff</property>
              <value>45</value>
          </less-than>
          <greater-than>
              <property>instrumentation/pfd/vf35-diff</property>
              <value>-45</value>
          </greater-than>
        </condition>
    </animation>

    <animation>
        <type>translate</type>
        <object-name>vf35-text</object-name>
        <property>instrumentation/pfd/vf35-diff</property>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>1</z>
        </axis>
        <factor>0.00092</factor>
    </animation>

<!-- Vref animation -->

    <animation>
        <type>select</type>
        <object-name>vref-text</object-name>
        <condition>
					<not>
            <property>gear/gear[1]/wow</property>
					</not>
          <less-than>
              <property>instrumentation/pfd/vref-diff</property>
              <value>45</value>
          </less-than>
          <greater-than>
              <property>instrumentation/pfd/vref-diff</property>
              <value>-45</value>
          </greater-than>
        </condition>
    </animation>

    <animation>
        <type>translate</type>
        <object-name>vref-text</object-name>
        <property>instrumentation/pfd/vref-diff</property>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>1</z>
        </axis>
        <factor>0.00092</factor>
    </animation>

<!-- Vmo animation -->

    <animation>
        <object-name>vmo</object-name>
        <property>instrumentation/pfd/vmo-diff</property>
        <type>textranslate</type>
        <axis>
            <x>0</x>
            <y>1</y>
        </axis>
        <factor>0.00420</factor>
    </animation>

<!-- Stall speed animation -->

    <animation>
        <object-name>v_stall</object-name>
        <property>instrumentation/pfd/stall-diff</property>
        <type>textranslate</type>
        <axis>
          <x>0</x>
          <y>1</y>
        </axis>
        <factor>-0.00420</factor>
    </animation>

    <animation>
        <type>select</type>
        <object-name>v_stall</object-name>
        <condition>
					<and>
            <property>instrumentation/pfd/stall-warning</property>
						<less-than>
			        <property>velocities/airspeed-kt</property>						
							<value>200</value>
						</less-than>
					</and>
        </condition>
    </animation>

<!-- hsi rose -->

    <animation>
        <type>texrotate</type>
        <object-name>PFD.hsi</object-name>
        <property>orientation/heading-magnetic-deg</property>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>-1</z>
        </axis>
        <center>
            <x>0.75</x>
            <y>0.25</y>
        </center>
    </animation>

<animation>
        <type>rotate</type>
        <object-name>hdg.bug</object-name>
        <property>autopilot/internal/heading-bug-error-deg</property>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>-0.002</y-m>
            <z-m>-0.0397</z-m>
        </center>
    </animation>

<animation>
        <type>rotate</type>
        <object-name>crs.needle</object-name>
        <object-name>crs.deflect</object-name>
        <object-name>to-flag</object-name>
        <object-name>from-flag</object-name>
        <property>autopilot/internal/course-offset</property>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>-0.002</y-m>
            <z-m>-0.0397</z-m>
        </center>
    </animation>

    <animation>
        <type>select</type>
        <object-name>to-flag</object-name>
        <condition>
            <property>autopilot/internal/to-flag</property>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>from-flag</object-name>
        <condition>
            <property>autopilot/internal/from-flag</property>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>ptr1</object-name>
        <condition>
            <greater-than>
                <property>instrumentation/primus2000/sc840/nav1ptr</property>
                <value>0</value>
            </greater-than>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>ptr1label-vor</object-name>
        <condition>
            <equals>
            <property>instrumentation/primus2000/sc840/nav1ptr</property>
            <value>1</value>
        </equals>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>ptr1label-adf</object-name>
        <condition>
            <equals>
            <property>instrumentation/primus2000/sc840/nav1ptr</property>
            <value>2</value>
        </equals>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>ptr1label-fms</object-name>
        <condition>
            <equals>
            <property>instrumentation/primus2000/sc840/nav1ptr</property>
            <value>3</value>
        </equals>
        </condition>
    </animation>

<animation>
        <type>rotate</type>
        <object-name>ptr1</object-name>
        <property>autopilot/internal/nav1-pointer</property>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>-0.002</y-m>
            <z-m>-0.0397</z-m>
        </center>
    </animation>

    <animation>
        <type>select</type>
        <object-name>ptr2</object-name>
        <condition>
            <greater-than>
                <property>instrumentation/primus2000/sc840/nav2ptr</property>
                <value>0</value>
            </greater-than>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>ptr2label-vor</object-name>
        <condition>
            <equals>
                <property>instrumentation/primus2000/sc840/nav2ptr</property>
                <value>1</value>
            </equals>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>ptr2label-adf</object-name>
        <condition>
            <equals>
                <property>instrumentation/primus2000/sc840/nav2ptr</property>
                <value>2</value>
            </equals>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>ptr2label-fms</object-name>
        <condition>
            <equals>
                <property>instrumentation/primus2000/sc840/nav2ptr</property>
