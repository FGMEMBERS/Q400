        <center>
            <x-m>0</x-m>
            <y-m>-0.002</y-m>
            <z-m>-0.0397</z-m>
        </center>
    </animation>

    <animation>
        <type>translate</type>
        <object-name>crs.deflect</object-name>
        <property>autopilot/internal/cdi</property>
        <interpolation>
            <entry><ind>-45.0</ind><dep>-0.01845</dep></entry>
            <entry><ind>0.0</ind><dep>0.0</dep></entry>
            <entry><ind>45.0</ind><dep>0.01845</dep></entry>
        </interpolation>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>-0.002</y-m>
            <z-m>-0.0397</z-m>
        </center>
    </animation>

<!-- end hsi rose -->

    <animation>
        <type>rotate</type>
        <object-name>vsi.needle</object-name>
