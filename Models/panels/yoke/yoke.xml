<?xml version="1.0"?>
<PropertyList>

    <path>yoke.ac</path>

<!-- Yoke L -->
<animation>
        <type>rotate</type>
        <object-name>YokeL_Elevator</object-name>
        <object-name>YokeL_Aileron</object-name>		
        <property>controls/flight/elevator</property>
        <factor>7.5</factor>
        <center>
            <x-m>-12.041177</x-m>
            <y-m>-0.499998</y-m>
            <z-m>1.356261</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>-1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
	  <object-name>YokeL_Aileron</object-name>
        <property>controls/flight/aileron</property>
        <factor>-45.0</factor>
        <center>
            <x-m>-11.951792</x-m>
            <y-m>-0.499998</y-m>
            <z-m>1.965158</z-m>
        </center>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

