      <lightmap-multi>1</lightmap-multi>
      <lightmap-factor n="0">
        <use>systems/electrical/outputs/lightmap/landing-light</use>
      </lightmap-factor>
      <!-- <lightmap-color type="vec3d" n="0"> 1.0 0.95 0.6 </lightmap-color> -->
