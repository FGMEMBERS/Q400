<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
	<path>frontpedestal.ac</path>
        
  

  <animation>
        <type>pick</type>
        <object-name>NDtcasL</object-name>
        <action>
            <button>0</button>
            <repeatable>0</repeatable>
            <binding>
                <command>property-toggle</command>
